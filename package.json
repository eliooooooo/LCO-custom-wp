{
	"name": "tailpress",
	"version": "3.4.0",
	"description": "Boilerplate WordPress theme with Tailwind CSS.",
	"author": "Jeffrey van Rossum",
	"repository": {
		"type": "git",
		"url": "https://github.com/jeffreyvr/tailpress"
	},
	"theme_uri": "https://github.com/jeffreyvr/tailpress",
	"author_uri": "https://vanrossum.dev",
	"text_domain": "tailpress",
	"license": "MIT",
	"scripts": {
		"clean": "rm -rf ./dist",
		"prod": "node ./scripts/esbuild.prod.js",
		"build": "node ./scripts/esbuild.build.js",
		"watch": "node ./scripts/esbuild.watch.js",
		"rebuild": "rm -rf ./node_modules && rm -rf package-lock.json && npm install && npm run build "
	},
	"devDependencies": {
		"@jeffreyvr/tailwindcss-tailpress": "^2.0.0",
		"autoprefixer": "^10.4.0",
		"browser-sync": "^2.26.14",
		"cross-env": "^6.0.3",
		"laravel-mix": "^6.0.29",
		"postcss": "^8.2.10",
		"postcss-import": "^14.0.0",
		"postcss-nested": "^5.0.3",
		"postcss-nested-ancestors": "^2.0.0",
		"resolve-url-loader": "^3.1.2",
		"tailwindcss": "^3.4.0"
	},
	"dependencies": {
		"alpinejs": "^3.14.0",
		"chokidar": "^3.6.0",
		"esbuild": "^0.21.4",
		"esbuild-style-plugin": "^1.6.3",
		"fs": "^0.0.1-security"
	}
}
